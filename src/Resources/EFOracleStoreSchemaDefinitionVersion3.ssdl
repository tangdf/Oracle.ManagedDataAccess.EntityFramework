<?xml version="1.0" encoding="utf-8"?>
<Schema Namespace="Oracle" Provider="Oracle.DataAccess.Client" ProviderManifestToken="9.2" Alias="Self" xmlns="http://schemas.microsoft.com/ado/2006/04/edm/ssdl">
  <EntityContainer Name="Schema">
    <EntitySet Name="STables" EntityType="Self.Table">
      <DefiningQuery>
      <!-- shsu: display objects owned by other schema by appending owner names. We select from 
      ALL_TABLES, instead of ALL_ALL_TABLES, because object tables are not supported at this time. -->
      <!-- MS OracleClient sample code
        SELECT
        '''' || TABLE_NAME || ''''    "Id"
        ,   CAST(NULL as varchar2(1)) "CatalogName"
        ,   sys_context('USERENV', 'CURRENT_USER') "SchemaName"
        ,   TABLE_NAME                "Name"
        FROM
        USER_TABLES
      -->
        SELECT
        '''' || TABLE_NAME || ''''    "Id"
        ,   CAST(NULL as varchar2(1)) "CatalogName"
        ,   OWNER                     "SchemaName"
        ,   TABLE_NAME                "Name"
        FROM ALL_TABLES
        WHERE OWNER IN (SYS_CONTEXT('USERENV', 'CURRENT_USER'))
      </DefiningQuery>
    </EntitySet>

    <EntitySet Name="STableColumns" EntityType="Self.TableColumn">
      <DefiningQuery>
        <!-- shsu: fixed for Oracle timestamp and interval types by stripping off precision -->
        <!-- and for interval types let precision = precision + scale if precision is less than scale -->
        <!-- MS OracleClient sample code
        SELECT
        '''' || c.TABLE_NAME || '_' || c.COLUMN_NAME || '''' "Id"
        , '''' || c.TABLE_NAME || '''' "ParentId"
        , c.COLUMN_NAME         "Name"
        , c.COLUMN_ID           "Ordinal"
        , CASE c.NULLABLE WHEN 'Y' THEN 1 ELSE 0 END "IsNullable"
        , LOWER(c.DATA_TYPE)    "TypeName"
        , c.DATA_LENGTH         "MaxLength"
        , c.DATA_PRECISION      "Precision"
        , c.DATA_PRECISION      "DateTimePrecision"
        , c.DATA_SCALE          "Scale"
        , CAST(NULL as varchar2(1)) "CollationCatalog"
        , CAST(NULL as varchar2(1)) "CollationSchema"
        , CAST(NULL as varchar2(1)) "CollationName"
        , CAST(NULL as varchar2(1)) "CharacterSetCatalog"
        , CAST(NULL as varchar2(1)) "CharacterSetSchema"
        , c.CHARACTER_SET_NAME  "CharacterSetName"
        , 0                     "IsMultiSet"
        , 0                     "IsIdentity"
        , 0                     "IsStoreGenerated"
        , c.DATA_DEFAULT        "Default"
        FROM
        USER_TAB_COLUMNS c
        INNER JOIN USER_OBJECTS o on o.OBJECT_NAME = c.TABLE_NAME
        AND o.OBJECT_TYPE = 'TABLE'
        -->
        SELECT
        '''' || c.TABLE_NAME || '_' || c.COLUMN_NAME || '''' "Id"
        , '''' || c.TABLE_NAME || '''' "ParentId"
        , c.COLUMN_NAME         "Name"
        , c.COLUMN_ID           "Ordinal"
        , CASE c.NULLABLE WHEN 'Y' THEN 1 ELSE 0 END "IsNullable"
        , (CASE WHEN INSTR(c.data_type, 'INTERVAL YEAR') != 0 THEN 'interval year to month'
        WHEN INSTR(c.data_type, 'INTERVAL DAY')  != 0 THEN 'interval day to second'
        WHEN INSTR(c.data_type, 'WITH TIME ZONE')  != 0  THEN 'timestamp with time zone'
        WHEN INSTR(c.data_type, 'WITH LOCAL TIME ZONE')  != 0 THEN 'timestamp with local time zone'
        WHEN INSTR(c.data_type, 'TIMESTAMP') != 0 AND INSTR(c.data_type, 'TIME ZONE') = 0 THEN 'timestamp'
        ELSE LOWER(c.data_type) END) "TypeName"
        , CASE c.data_type
        WHEN 'CHAR' THEN c.char_length
        WHEN 'VARCHAR2' THEN c.char_length
        WHEN 'NCHAR' THEN c.char_length
        WHEN 'NVARCHAR2' THEN c.char_length
        ELSE c.DATA_LENGTH END       "MaxLength"
        , (CASE WHEN INSTR(c.data_type, 'INTERVAL YEAR') != 0 AND c.DATA_PRECISION &lt; c.DATA_SCALE THEN c.DATA_SCALE
        WHEN INSTR(c.data_type, 'INTERVAL DAY') != 0 AND c.DATA_PRECISION &lt; c.DATA_SCALE THEN c.DATA_SCALE
        ELSE c.DATA_PRECISION END)       "Precision"
        , c.DATA_PRECISION      "DateTimePrecision"
        , c.DATA_SCALE          "Scale"
        , CAST(NULL as varchar2(1)) "CollationCatalog"
        , CAST(NULL as varchar2(1)) "CollationSchema"
        , CAST(NULL as varchar2(1)) "CollationName"
        , CAST(NULL as varchar2(1)) "CharacterSetCatalog"
        , CAST(NULL as varchar2(1)) "CharacterSetSchema"
        , c.CHARACTER_SET_NAME  "CharacterSetName"
        , 0                     "IsMultiSet"
        , 0                     "IsIdentity"
        , 0                     "IsStoreGenerated"
        , c.DATA_DEFAULT        "Default"
        FROM
        ALL_TAB_COLUMNS c, ALL_OBJECTS o
        WHERE o.OBJECT_NAME = c.TABLE_NAME
        AND o.OBJECT_TYPE = 'TABLE'
        AND o.OWNER = c.OWNER
        AND o.OWNER IN (SYS_CONTEXT('USERENV', 'CURRENT_USER'))
      </DefiningQuery>
    </EntitySet>
    
    <EntitySet Name="SViews" EntityType="Self.View">
      <DefiningQuery>
        <!-- MS OracleClient sample code
        SELECT
        '''' ||  VIEW_NAME || ''''    "Id"
        ,   CAST(NULL as varchar2(1)) "CatalogName"
        ,   sys_context('USERENV', 'CURRENT_USER') "SchemaName"
        ,   VIEW_NAME                 "Name"
        ,   TEXT                      "ViewDefinition"
        ,   CASE VIEW_TYPE WHEN 'MATERIALIZED' THEN 1 ELSE 0 END "IsUpdatable"
        FROM
        USER_VIEWS
        -->
        SELECT
        '''' ||  VIEW_NAME || ''''    "Id"
        ,   CAST(NULL as varchar2(1)) "CatalogName"
        ,   OWNER                     "SchemaName"
        ,   VIEW_NAME                 "Name"
        ,   TEXT                      "ViewDefinition"
        ,   CASE VIEW_TYPE WHEN 'MATERIALIZED' THEN 1 ELSE 0 END "IsUpdatable"
        FROM ALL_VIEWS
        WHERE OWNER IN (SYS_CONTEXT('USERENV', 'CURRENT_USER'))
      </DefiningQuery>
    </EntitySet>
    
    <EntitySet Name="SViewColumns" EntityType="Self.ViewColumn">
      <DefiningQuery>
        <!-- shsu: fixed for Oracle timestamp and interval types by stripping off precision -->
        <!-- and for interval types let precision = precision + scale if precision is less than scale -->
        <!-- MS OracleClient sample code
        SELECT
        '''' || c.TABLE_NAME || '_' || c.COLUMN_NAME || '''' "Id"
        , '''' || c.TABLE_NAME || '''' "ParentId"
        , c.COLUMN_NAME         "Name"
        , c.COLUMN_ID           "Ordinal"
        , CASE c.NULLABLE WHEN 'Y' THEN 1 ELSE 0 END "IsNullable"
        , LOWER(c.DATA_TYPE)    "TypeName"
        , c.DATA_LENGTH         "MaxLength"
        , c.DATA_PRECISION      "Precision"
        , c.DATA_PRECISION      "DateTimePrecision"
        , c.DATA_SCALE          "Scale"
        , CAST(NULL as varchar2(1)) "CollationCatalog"
        , CAST(NULL as varchar2(1)) "CollationSchema"
        , CAST(NULL as varchar2(1)) "CollationName"
        , CAST(NULL as varchar2(1)) "CharacterSetCatalog"
        , CAST(NULL as varchar2(1)) "CharacterSetSchema"
        , c.CHARACTER_SET_NAME  "CharacterSetName"
        , 0                     "IsMultiSet"
        , 0                     "IsIdentity"
        , 0                     "IsStoreGenerated"
        , c.DATA_DEFAULT        "Default"
        FROM
        USER_TAB_COLUMNS c
        INNER JOIN USER_OBJECTS o on o.OBJECT_NAME = c.TABLE_NAME
        AND o.OBJECT_TYPE = 'VIEW'
        -->
        SELECT
        '''' || c.TABLE_NAME || '_' || c.COLUMN_NAME || '''' "Id"
        , '''' || c.TABLE_NAME || '''' "ParentId"
        , c.COLUMN_NAME         "Name"
        , c.COLUMN_ID           "Ordinal"
        , CASE c.NULLABLE WHEN 'Y' THEN 1 ELSE 0 END "IsNullable"
        , (CASE WHEN INSTR(c.data_type, 'INTERVAL YEAR') != 0 THEN 'interval year to month'
        WHEN INSTR(c.data_type, 'INTERVAL DAY') != 0 THEN 'interval day to second'
        WHEN INSTR(c.data_type, 'WITH TIME ZONE') != 0  THEN 'timestamp with time zone'
        WHEN INSTR(c.data_type, 'WITH LOCAL TIME ZONE') != 0 THEN 'timestamp with local time zone'
        WHEN INSTR(c.data_type, 'TIMESTAMP') != 0 AND INSTR(c.data_type, 'TIME ZONE') = 0 THEN 'timestamp'
        ELSE LOWER(c.data_type) END)  "TypeName"
        , CASE c.data_type
        WHEN 'CHAR' THEN c.char_length
        WHEN 'VARCHAR2' THEN c.char_length
        WHEN 'NCHAR' THEN c.char_length
        WHEN 'NVARCHAR2' THEN c.char_length
        ELSE c.DATA_LENGTH END       "MaxLength"
        , (CASE WHEN INSTR(c.data_type, 'INTERVAL YEAR') != 0 AND c.DATA_PRECISION &lt; c.DATA_SCALE THEN c.DATA_SCALE
        WHEN INSTR(c.data_type, 'INTERVAL DAY') != 0 AND c.DATA_PRECISION &lt; c.DATA_SCALE THEN c.DATA_SCALE
        ELSE c.DATA_PRECISION END)       "Precision"
        , c.DATA_PRECISION      "DateTimePrecision"
        , c.DATA_SCALE          "Scale"
        , CAST(NULL as varchar2(1)) "CollationCatalog"
        , CAST(NULL as varchar2(1)) "CollationSchema"
        , CAST(NULL as varchar2(1)) "CollationName"
        , CAST(NULL as varchar2(1)) "CharacterSetCatalog"
        , CAST(NULL as varchar2(1)) "CharacterSetSchema"
        , c.CHARACTER_SET_NAME  "CharacterSetName"
        , 0                     "IsMultiSet"
        , 0                     "IsIdentity"
        , 0                     "IsStoreGenerated"
        , c.DATA_DEFAULT        "Default"
        FROM ALL_TAB_COLUMNS c, ALL_OBJECTS o
        WHERE o.OBJECT_NAME = c.TABLE_NAME
        AND o.OBJECT_TYPE = 'VIEW'
        AND o.OWNER = c.OWNER
        AND o.OWNER IN (SYS_CONTEXT('USERENV', 'CURRENT_USER'))
      </DefiningQuery>
    </EntitySet>

    <EntitySet Name="SFunctions" EntityType="Self.Function">
      <DefiningQuery>
        <!-- shsu: 1. fixed SchemaName and format for functions within packages 
                   2. excluded functions that return ref cursor and let it appear like procedure. See SProcedures. -->
        <!-- MS OracleClient sample code
        SELECT
        '''' || ret.PACKAGE_NAME || '_' || func.OBJECT_NAME || ''''	"Id"
        ,	CAST(NULL as varchar2(1)) "CatalogName"
        ,	(CASE WHEN ret.PACKAGE_NAME IS NULL THEN sys_context('USERENV', 'CURRENT_USER') ELSE ret.PACKAGE_NAME END) "SchemaName"
        ,	func.OBJECT_NAME    "Name"
        ,	LOWER(ret.DATA_TYPE)"ReturnTypeName"
        ,	ret.DATA_LENGTH     "ReturnMaxLength"
        ,	ret.DATA_PRECISION	"ReturnPrecision"
        ,	ret.DATA_PRECISION	"ReturnDateTimePrecision"
        ,	ret.DATA_SCALE		  "ReturnScale"
        ,	CAST(NULL as varchar2(1)) "ReturnCollationCatalog"
        ,	CAST(NULL as varchar2(1)) "ReturnCollationSchema"
        ,	CAST(NULL as varchar2(1)) "ReturnCollationName"
        ,	CAST(NULL as varchar2(1)) "ReturnCharacterSetCatalog"
        ,	CAST(NULL as varchar2(1)) "ReturnCharacterSetSchema"
        ,	ret.CHARACTER_SET_NAME	"ReturnCharacterSetName"
        ,	0	                  "ReturnIsMultiSet"
        ,	0	                  "IsAggregate"
        ,	0	                  "IsBuiltIn"
        ,	0	                  "IsNiladic"
        FROM
        USER_PROCEDURES func
        INNER JOIN USER_OBJECTS obj ON obj.OBJECT_NAME = func.OBJECT_NAME
        AND obj.OBJECT_TYPE = 'FUNCTION'
        INNER JOIN USER_ARGUMENTS ret on ret.OBJECT_NAME = func.OBJECT_NAME
        AND ret.POSITION = 0
        -->
        SELECT
        '''' || (CASE WHEN PACKAGE_NAME IS NOT NULL THEN PACKAGE_NAME || '.' || OBJECT_NAME ELSE OBJECT_NAME END) || ''''	"Id"
        ,	CAST(NULL as varchar2(1)) "CatalogName"
        , OWNER                     "SchemaName"
        ,	(CASE WHEN PACKAGE_NAME IS NOT NULL THEN PACKAGE_NAME || '.' || OBJECT_NAME ELSE OBJECT_NAME END) "Name"
        ,	LOWER(DATA_TYPE) "ReturnTypeName"
        ,	DATA_LENGTH     "ReturnMaxLength"
        ,	DATA_PRECISION	"ReturnPrecision"
        ,	DATA_PRECISION	"ReturnDateTimePrecision"
        ,	DATA_SCALE		  "ReturnScale"
        ,	CAST(NULL as varchar2(1)) "ReturnCollationCatalog"
        ,	CAST(NULL as varchar2(1)) "ReturnCollationSchema"
        ,	CAST(NULL as varchar2(1)) "ReturnCollationName"
        ,	CAST(NULL as varchar2(1)) "ReturnCharacterSetCatalog"
        ,	CAST(NULL as varchar2(1)) "ReturnCharacterSetSchema"
        ,	CHARACTER_SET_NAME	"ReturnCharacterSetName"
        ,	0	                  "ReturnIsMultiSet"
        ,	0	                  "IsAggregate"
        ,	0	                  "IsBuiltIn"
        ,	0	                  "IsNiladic"
        , 0                   "IsTvf"
        FROM ALL_ARGUMENTS A
        WHERE DATA_LEVEL = 0
        AND (SEQUENCE = 1 AND POSITION = 0) AND LOWER(DATA_TYPE) != 'ref cursor'
        AND ((PACKAGE_NAME IS NULL or EXISTS (select 'x' from all_objects O where A.PACKAGE_NAME = O.OBJECT_NAME AND A.OWNER=O.OWNER AND O.OBJECT_TYPE='PACKAGE')))
        AND (OVERLOAD IS NULL OR OVERLOAD = '1')
        AND A.OWNER IN (SYS_CONTEXT('USERENV', 'CURRENT_USER'))
        <!--shsu:old
        SELECT
        '''' || func.OBJECT_NAME || ''''	"Id"
        ,	CAST(NULL as varchar2(1)) "CatalogName"
        , func.OWNER                "SchemaName"
        ,	func.OBJECT_NAME          "Name"
        ,	LOWER(ret.DATA_TYPE)"ReturnTypeName"
        ,	ret.DATA_LENGTH     "ReturnMaxLength"
        ,	ret.DATA_PRECISION	"ReturnPrecision"
        ,	ret.DATA_PRECISION	"ReturnDateTimePrecision"
        ,	ret.DATA_SCALE		  "ReturnScale"
        ,	CAST(NULL as varchar2(1)) "ReturnCollationCatalog"
        ,	CAST(NULL as varchar2(1)) "ReturnCollationSchema"
        ,	CAST(NULL as varchar2(1)) "ReturnCollationName"
        ,	CAST(NULL as varchar2(1)) "ReturnCharacterSetCatalog"
        ,	CAST(NULL as varchar2(1)) "ReturnCharacterSetSchema"
        ,	ret.CHARACTER_SET_NAME	"ReturnCharacterSetName"
        ,	0	                  "ReturnIsMultiSet"
        ,	0	                  "IsAggregate"
        ,	0	                  "IsBuiltIn"
        ,	0	                  "IsNiladic"
        FROM
        ALL_PROCEDURES func
        INNER JOIN ALL_OBJECTS obj ON obj.OBJECT_NAME = func.OBJECT_NAME
        AND obj.OBJECT_TYPE = 'FUNCTION' AND func.OWNER NOT IN ('SYS','CTXSYS','SYSTEM','EXFSYS','ORDSYS','XDB','ORDDATA','WMSYS','ORDPLUGINS','MDSYS','OLAPSYS','WKSYS')
        INNER JOIN ALL_ARGUMENTS ret on ret.OBJECT_NAME = func.OBJECT_NAME
        AND ret.POSITION = 0 AND ret.OWNER NOT IN ('SYS','CTXSYS','SYSTEM','EXFSYS','ORDSYS','XDB','ORDDATA','WMSYS','ORDPLUGINS','MDSYS','OLAPSYS','WKSYS')
        UNION SELECT
        '''' || ar.PACKAGE_NAME || '.' || OBJECT_NAME || ''''	"Id"
        ,	CAST(NULL as varchar2(1)) "CatalogName"
        ,	ar.OWNER                  "SchemaName"
        ,   ar.PACKAGE_NAME || '.' || OBJECT_NAME   "Name"
        ,	LOWER(ar.DATA_TYPE)"ReturnTypeName"
        ,	DATA_LENGTH     "ReturnMaxLength"
        ,	DATA_PRECISION	"ReturnPrecision"
        ,	DATA_PRECISION	"ReturnDateTimePrecision"
        ,	DATA_SCALE		  "ReturnScale"
        ,	CAST(NULL as varchar2(1)) "ReturnCollationCatalog"
        ,	CAST(NULL as varchar2(1)) "ReturnCollationSchema"
        ,	CAST(NULL as varchar2(1)) "ReturnCollationName"
        ,	CAST(NULL as varchar2(1)) "ReturnCharacterSetCatalog"
        ,	CAST(NULL as varchar2(1)) "ReturnCharacterSetSchema"
        ,	ar.CHARACTER_SET_NAME	"ReturnCharacterSetName"
        ,	0	                  "ReturnIsMultiSet"
        ,	0	                  "IsAggregate"
        ,	0	                  "IsBuiltIn"
        ,	0	                  "IsNiladic"
        FROM ALL_ARGUMENTS ar WHERE POSITION = 0 and PACKAGE_NAME IS NOT NULL
        AND (ar.OVERLOAD IS NULL OR ar.OVERLOAD = '1')
        AND ar.OWNER NOT IN ('SYS','CTXSYS','SYSTEM','EXFSYS','ORDSYS','XDB','ORDDATA','WMSYS','ORDPLUGINS','MDSYS','OLAPSYS','WKSYS')
        -->
      </DefiningQuery>
    </EntitySet>
    
    <EntitySet Name="SFunctionParameters" EntityType="Self.Parameter">
      <DefiningQuery>
        <!-- shsu: 1. fixed format and inout parameter issues for standalone functions and functions within packages 
                   2. excluded ref cursor return type and parameters to support implicit ref cursor -->
        <!-- MS OracleClient sample code
        SELECT
        '''' || arg.OBJECT_NAME || '_' || arg.ARGUMENT_NAME || ''''	"Id"
        ,	'''' || arg.PACKAGE_NAME || '_' || func.OBJECT_NAME || ''''	"ParentId"
        ,	arg.ARGUMENT_NAME   "Name"
        ,	arg.POSITION	      "Ordinal"
        ,	LOWER(arg.DATA_TYPE)"TypeName"
        ,	arg.DATA_LENGTH     "MaxLength"
        ,	arg.DATA_PRECISION  "Precision"
        ,	arg.DATA_PRECISION	"DateTimePrecision"
        ,	arg.DATA_SCALE	    "Scale"
        ,	CAST(NULL as varchar2(1)) "CollationCatalog"
        ,	CAST(NULL as varchar2(1)) "CollationSchema"
        ,	CAST(NULL as varchar2(1)) "CollationName"
        ,	CAST(NULL as varchar2(1)) "CharacterSetCatalog"
        ,	CAST(NULL as varchar2(1)) "CharacterSetSchema"
        ,	arg.CHARACTER_SET_NAME	"CharacterSetName"
        ,	0	                  "IsMultiSet"
        ,	arg.IN_OUT	        "Mode"
        ,	arg.DEFAULT_VALUE   "Default"
        FROM
        USER_ARGUMENTS arg
        INNER JOIN USER_OBJECTS func ON func.OBJECT_ID = arg.OBJECT_ID
        AND func.OBJECT_TYPE = 'FUNCTION'
        WHERE
        arg.POSITION &lt;&gt; 0
        -->
        SELECT
        '''' || AA.OBJECT_NAME || '_' || ARGUMENT_NAME || ''''	"Id"
        ,'''' || (CASE WHEN AA.PACKAGE_NAME IS NOT NULL THEN AA.PACKAGE_NAME || '.' || AA.OBJECT_NAME ELSE AA.OBJECT_NAME END) || ''''   "ParentId"
        , ARGUMENT_NAME   "Name"
        ,	POSITION	      "Ordinal"
        ,	LOWER(DATA_TYPE)"TypeName"
        ,	DATA_LENGTH     "MaxLength"
        ,	DATA_PRECISION  "Precision"
        ,	DATA_PRECISION	"DateTimePrecision"
        ,	DATA_SCALE	"Scale"
        ,	CAST(NULL as varchar2(1)) "CollationCatalog"
        ,	CAST(NULL as varchar2(1)) "CollationSchema"
        ,	CAST(NULL as varchar2(1)) "CollationName"
        ,	CAST(NULL as varchar2(1)) "CharacterSetCatalog"
        ,	CAST(NULL as varchar2(1)) "CharacterSetSchema"
        ,	CHARACTER_SET_NAME	  "CharacterSetName"
        ,	0	                    "IsMultiSet"
        , (CASE WHEN IN_OUT = 'IN/OUT' THEN 'INOUT' ELSE IN_OUT END) "Mode"
        ,	DEFAULT_VALUE   "Default"
        FROM ALL_ARGUMENTS AA,
        (SELECT A.OWNER, PACKAGE_NAME, A.OBJECT_NAME
        FROM ALL_ARGUMENTS A
        WHERE DATA_LEVEL = 0
        AND (SEQUENCE = 1 AND POSITION = 0) AND LOWER(DATA_TYPE) != 'ref cursor'
        AND ((PACKAGE_NAME IS NULL or EXISTS (select 'x' from all_objects O where A.PACKAGE_NAME = O.OBJECT_NAME AND A.OWNER=O.OWNER AND O.OBJECT_TYPE='PACKAGE')))
        AND (OVERLOAD IS NULL OR OVERLOAD = '1')
        AND A.OWNER IN (SYS_CONTEXT('USERENV', 'CURRENT_USER'))) NEWT
        WHERE DATA_LEVEL = 0 AND (AA.PACKAGE_NAME IS NULL OR AA.PACKAGE_NAME = NEWT.PACKAGE_NAME)
        AND AA.OBJECT_NAME = NEWT.OBJECT_NAME
        AND AA.OWNER = NEWT.OWNER
        AND AA.OWNER IN (SYS_CONTEXT('USERENV', 'CURRENT_USER'))
        AND (OVERLOAD IS NULL OR OVERLOAD = '1')
        AND SEQUENCE &gt; 1
        AND POSITION &gt; 0
        AND LOWER(DATA_TYPE) != 'ref cursor'
        <!-- shsu:old
        SELECT
        '''' || arg.OBJECT_NAME || '_' || arg.ARGUMENT_NAME || ''''	"Id"
        ,	'''' || func.OBJECT_NAME || ''''	                        "ParentId"
        ,	arg.ARGUMENT_NAME   "Name"
        ,	arg.POSITION	  "Ordinal"
        ,	LOWER(arg.DATA_TYPE)"TypeName"
        ,	arg.DATA_LENGTH     "MaxLength"
        ,	arg.DATA_PRECISION  "Precision"
        ,	arg.DATA_PRECISION	"DateTimePrecision"
        ,	arg.DATA_SCALE	    "Scale"
        ,	CAST(NULL as varchar2(1)) "CollationCatalog"
        ,	CAST(NULL as varchar2(1)) "CollationSchema"
        ,	CAST(NULL as varchar2(1)) "CollationName"
        ,	CAST(NULL as varchar2(1)) "CharacterSetCatalog"
        ,	CAST(NULL as varchar2(1)) "CharacterSetSchema"
        ,	arg.CHARACTER_SET_NAME	"CharacterSetName"
        ,	0	                  "IsMultiSet"
        , (CASE WHEN INSTR(UPPER(arg.IN_OUT), 'IN/OUT') != 0 THEN 'INOUT' ELSE arg.IN_OUT END) "Mode"
        ,	arg.DEFAULT_VALUE   "Default"
        FROM
        ALL_ARGUMENTS arg
        INNER JOIN ALL_OBJECTS func ON func.OBJECT_ID = arg.OBJECT_ID
        AND func.OBJECT_TYPE = 'FUNCTION'
        WHERE
        arg.POSITION != 0 AND arg.OWNER NOT IN ('SYS','CTXSYS','SYSTEM','EXFSYS','ORDSYS','XDB','ORDDATA','WMSYS','ORDPLUGINS','MDSYS','OLAPSYS','WKSYS')
        UNION ALL
        SELECT
        '''' || PACKAGE_NAME || '.' || OBJECT_NAME || '_' || ARGUMENT_NAME || ''''	"Id"
        ,	'''' || PACKAGE_NAME || '.' || OBJECT_NAME || ''''	"ParentId"
        ,	ARGUMENT_NAME   "Name"
        ,	POSITION	      "Ordinal"
        ,	LOWER(DATA_TYPE)"TypeName"
        ,	DATA_LENGTH     "MaxLength"
        ,	DATA_PRECISION  "Precision"
        ,	DATA_PRECISION	"DateTimePrecision"
        ,	DATA_SCALE	    "Scale"
        ,	CAST(NULL as varchar2(1)) "CollationCatalog"
        ,	CAST(NULL as varchar2(1)) "CollationSchema"
        ,	CAST(NULL as varchar2(1)) "CollationName"
        ,	CAST(NULL as varchar2(1)) "CharacterSetCatalog"
        ,	CAST(NULL as varchar2(1)) "CharacterSetSchema"
        ,	CHARACTER_SET_NAME	"CharacterSetName"
        ,	0	                  "IsMultiSet"
        , (CASE WHEN INSTR(UPPER(IN_OUT), 'IN/OUT') != 0 THEN 'INOUT' ELSE IN_OUT END) "Mode"
        ,	DEFAULT_VALUE   "Default"
        FROM ALL_ARGUMENTS WHERE POSITION != 0 AND PACKAGE_NAME IS NOT NULL AND
        OBJECT_NAME IN (SELECT OBJECT_NAME FROM ALL_ARGUMENTS WHERE POSITION = 0
        AND DATA_TYPE IS NOT NULL AND PACKAGE_NAME IS NOT NULL)
        AND (OVERLOAD IS NULL OR OVERLOAD = '1')
        AND OWNER NOT IN ('SYS','CTXSYS','SYSTEM','EXFSYS','ORDSYS','XDB','ORDDATA','WMSYS','ORDPLUGINS','MDSYS','OLAPSYS','WKSYS')
        -->
      </DefiningQuery>
    </EntitySet>

    <EntitySet Name="SFunctionReturnTableColumns" EntityType="Self.FunctionReturnTableColumn">
      <DefiningQuery>
        SELECT
        NULL "Id"
        , NULL "ParentId"
        , NULL "Name"
        , 0 "Ordinal"
        , 0 "IsNullable"
        , NULL "TypeName"
        , 0 "MaxLength"
        , 0 "Precision"
        , 0 "DateTimePrecision"
        , 0 "Scale"
        , NULL "CollationCatalog"
        , NULL "CollationSchema"
        , NULL "CollationName"
        , NULL "CharacterSetCatalog"
