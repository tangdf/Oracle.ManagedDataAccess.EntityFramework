    <EntitySet Name="SProcedures" EntityType="Self.Procedure">
      <DefiningQuery>
        <!-- shsu: 1. fixed SchemaName and format for standalone procedures and procedures within packages 
                   2. included functions that return ref cursor so that they can be used in Function Import. -->
        <!-- MS OracleClient sample code
        SELECT
        '''' || (CASE WHEN proc.PROCEDURE_NAME IS NULL THEN '' ELSE proc.OBJECT_NAME END) || '_'|| (CASE WHEN proc.PROCEDURE_NAME IS NULL THEN proc.OBJECT_NAME ELSE proc.PROCEDURE_NAME END) || '''' as "Id"
        , CAST(NULL as varchar2(1)) "CatalogName"
        , CASE WHEN proc.PROCEDURE_NAME IS NOT NULL THEN proc.PROCEDURE_NAME WHEN proc.OBJECT_NAME IS NULL THEN sys_context('USERENV', 'CURRENT_USER') ELSE proc.OBJECT_NAME END "SchemaName"
        , CASE WHEN proc.PROCEDURE_NAME IS NULL THEN proc.OBJECT_NAME ELSE proc.PROCEDURE_NAME END "Name"
        FROM
        USER_PROCEDURES proc
        INNER JOIN USER_OBJECTS obj ON proc.OBJECT_NAME = obj.OBJECT_NAME
        AND (obj.OBJECT_TYPE = 'PROCEDURE' OR obj.OBJECT_TYPE = 'PACKAGE')
        -->
        SELECT
        '''' || (CASE WHEN PACKAGE_NAME IS NOT NULL THEN PACKAGE_NAME || '.' || OBJECT_NAME ELSE OBJECT_NAME END) || ''''	"Id"
        ,	CAST(NULL as varchar2(1)) "CatalogName"
        , OWNER                     "SchemaName"
        ,	(CASE WHEN PACKAGE_NAME IS NOT NULL THEN PACKAGE_NAME || '.' || OBJECT_NAME ELSE OBJECT_NAME END) "Name"
        FROM ALL_ARGUMENTS A
        WHERE DATA_LEVEL = 0
        AND ((SEQUENCE = 0 OR (SEQUENCE = 1 AND POSITION &gt; 0)) OR (SEQUENCE = 1 AND POSITION = 0 AND LOWER(DATA_TYPE) = 'ref cursor'))
        AND ((PACKAGE_NAME IS NULL or EXISTS (select 'x' from all_objects O where A.PACKAGE_NAME = O.OBJECT_NAME AND A.OWNER=O.OWNER AND O.OBJECT_TYPE='PACKAGE')))
        AND (OVERLOAD IS NULL OR OVERLOAD = '1')
        AND A.OWNER IN (SYS_CONTEXT('USERENV', 'CURRENT_USER'))
        <!--shsu:old
        SELECT
        '''' || proc.OBJECT_NAME || '''' as "Id"
        , CAST(NULL as varchar2(1)) "CatalogName"
        , PROC.OWNER                "SchemaName"
        , proc.OBJECT_NAME          "Name"
        FROM
        ALL_PROCEDURES proc
        INNER JOIN ALL_OBJECTS obj ON obj.OBJECT_NAME = proc.OBJECT_NAME
        AND obj.OBJECT_TYPE = 'PROCEDURE' AND proc.OWNER NOT IN ('SYS','CTXSYS','SYSTEM','EXFSYS','ORDSYS','XDB','ORDDATA','WMSYS','ORDPLUGINS','MDSYS','OLAPSYS','WKSYS')
        INNER JOIN ALL_ARGUMENTS ret on ret.OBJECT_NAME = proc.OBJECT_NAME
        AND ret.POSITION > 0 AND ret.OWNER NOT IN ('SYS','CTXSYS','SYSTEM','EXFSYS','ORDSYS','XDB','ORDDATA','WMSYS','ORDPLUGINS','MDSYS','OLAPSYS','WKSYS')
        UNION
        SELECT DISTINCT
        '''' || ar.PACKAGE_NAME || '.' || proc.PROCEDURE_NAME || ''''	"Id"
        , CAST(NULL as varchar2(1)) "CatalogName"
        , ar.OWNER                  "SchemaName"
        , ar.PACKAGE_NAME || '.' || ar.OBJECT_NAME                      "Name"
        FROM
        ALL_ARGUMENTS ar
        INNER JOIN ALL_PROCEDURES proc ON ar.OBJECT_NAME = proc.PROCEDURE_NAME
        WHERE PACKAGE_NAME IS NOT NULL AND
        ar.OBJECT_NAME NOT IN
        (SELECT OBJECT_NAME FROM ALL_ARGUMENTS WHERE POSITION = 0
        AND DATA_TYPE IS NOT NULL AND PACKAGE_NAME IS NOT NULL)
        AND (ar.OVERLOAD IS NULL OR ar.OVERLOAD = '1')
        AND ar.OWNER NOT IN ('SYS','CTXSYS','SYSTEM','EXFSYS','ORDSYS','XDB','ORDDATA','WMSYS','ORDPLUGINS','MDSYS','OLAPSYS','WKSYS')
        -->
      </DefiningQuery>
    </EntitySet>
    
    <EntitySet Name="SProcedureParameters" EntityType="Self.Parameter">
      <DefiningQuery>
        <!-- shsu: 1. fixed format and inout parameter issues for standalone procedures and procedures within packages 
                   2. excluded ref cursor parameters to support implicit ref cursor -->
        <!-- MS OracleClient sample code
        SELECT
        '''' || arg.OBJECT_NAME || '_' || arg.ARGUMENT_NAME || ''''	"Id"
        ,	'''' || arg.PACKAGE_NAME || '_' || arg.OBJECT_NAME || ''''	"ParentId"
        ,	arg.ARGUMENT_NAME       "Name"
        ,	arg.POSITION	          "Ordinal"
        ,	LOWER(arg.DATA_TYPE)	  "TypeName"
        ,	arg.DATA_LENGTH         "MaxLength"
        ,	arg.DATA_PRECISION      "Precision"
        ,	arg.DATA_PRECISION	    "DateTimePrecision"
        ,	arg.DATA_SCALE	        "Scale"
        ,	CAST(NULL as varchar2(1)) "CollationCatalog"
        ,	CAST(NULL as varchar2(1)) "CollationSchema"
        ,	CAST(NULL as varchar2(1)) "CollationName"
        ,	CAST(NULL as varchar2(1)) "CharacterSetCatalog"
        ,	CAST(NULL as varchar2(1)) "CharacterSetSchema"
        ,	arg.CHARACTER_SET_NAME	"CharacterSetName"
        ,	0	                      "IsMultiSet"
        ,	arg.IN_OUT	            "Mode"
        ,	arg.DEFAULT_VALUE       "Default"
        FROM
        USER_ARGUMENTS arg
        INNER JOIN USER_OBJECTS obj ON arg.OBJECT_ID = obj.OBJECT_ID
        AND (obj.OBJECT_TYPE = 'PROCEDURE' OR obj.OBJECT_TYPE = 'PACKAGE')
        -->
        SELECT
        '''' || AA.OBJECT_NAME || '_' || ARGUMENT_NAME || ''''	"Id"
        ,	'''' || (CASE WHEN AA.PACKAGE_NAME IS NOT NULL THEN AA.PACKAGE_NAME || '.' || AA.OBJECT_NAME ELSE AA.OBJECT_NAME END) || ''''	"ParentId"
        ,	ARGUMENT_NAME   "Name"
        ,	POSITION	      "Ordinal"
        ,	LOWER(DATA_TYPE) "TypeName"
        ,	DATA_LENGTH     "MaxLength"
        ,	DATA_PRECISION  "Precision"
        ,	DATA_PRECISION	"DateTimePrecision"
        ,	DATA_SCALE	    "Scale"
        ,	CAST(NULL as varchar2(1)) "CollationCatalog"
        ,	CAST(NULL as varchar2(1)) "CollationSchema"
        ,	CAST(NULL as varchar2(1)) "CollationName"
        ,	CAST(NULL as varchar2(1)) "CharacterSetCatalog"
        ,	CAST(NULL as varchar2(1)) "CharacterSetSchema"
        ,	CHARACTER_SET_NAME	"CharacterSetName"
        ,	0	                  "IsMultiSet"
        , (CASE WHEN IN_OUT = 'IN/OUT' THEN 'INOUT' ELSE IN_OUT END) "Mode"
        ,	DEFAULT_VALUE   "Default"
        FROM ALL_ARGUMENTS AA,
        (SELECT A.OWNER,
        A.OBJECT_NAME,
        A.PACKAGE_NAME
        FROM ALL_ARGUMENTS A
        WHERE DATA_LEVEL = 0
        AND (SEQUENCE = 0 OR (SEQUENCE = 1 AND POSITION &gt; 0)) AND LOWER(DATA_TYPE) != 'ref cursor'
        AND ((PACKAGE_NAME IS NULL or EXISTS (select 'x' from all_objects O where A.PACKAGE_NAME = O.OBJECT_NAME AND A.OWNER=O.OWNER AND O.OBJECT_TYPE='PACKAGE')))
        AND (OVERLOAD IS NULL OR OVERLOAD = '1')
        AND A.OWNER IN (SYS_CONTEXT('USERENV', 'CURRENT_USER'))) NEWT
        WHERE DATA_LEVEL = 0
        AND (SEQUENCE = 0 OR (SEQUENCE &gt;= 1 and POSITION &gt; 0)) AND LOWER(DATA_TYPE) != 'ref cursor'
        AND AA.OWNER IN (SYS_CONTEXT('USERENV', 'CURRENT_USER'))
        AND AA.OWNER = NEWT.OWNER
        AND AA.OBJECT_NAME = NEWT.OBJECT_NAME
        AND (AA.PACKAGE_NAME IS NULL OR AA.PACKAGE_NAME = NEWT.PACKAGE_NAME)
        AND (AA.OVERLOAD IS NULL OR AA.OVERLOAD = '1')
        <!--shsu:old
        SELECT
        '''' || arg.OBJECT_NAME || '_' || arg.ARGUMENT_NAME || ''''	"Id"
        ,	'''' || (CASE WHEN arg.PACKAGE_NAME IS NOT NULL THEN arg.PACKAGE_NAME || '.' || func.OBJECT_NAME ELSE func.OBJECT_NAME END) || ''''	"ParentId"
        ,	arg.ARGUMENT_NAME   "Name"
        ,	arg.POSITION	      "Ordinal"
        ,	LOWER(arg.DATA_TYPE)"TypeName"
        ,	arg.DATA_LENGTH     "MaxLength"
        ,	arg.DATA_PRECISION  "Precision"
        ,	arg.DATA_PRECISION	"DateTimePrecision"
        ,	arg.DATA_SCALE	    "Scale"
        ,	CAST(NULL as varchar2(1)) "CollationCatalog"
        ,	CAST(NULL as varchar2(1)) "CollationSchema"
        ,	CAST(NULL as varchar2(1)) "CollationName"
        ,	CAST(NULL as varchar2(1)) "CharacterSetCatalog"
        ,	CAST(NULL as varchar2(1)) "CharacterSetSchema"
        ,	arg.CHARACTER_SET_NAME	"CharacterSetName"
        ,	0	                  "IsMultiSet"
        , (CASE WHEN INSTR(UPPER(arg.IN_OUT), 'IN/OUT') != 0 THEN 'INOUT' ELSE arg.IN_OUT END) "Mode"
        ,	arg.DEFAULT_VALUE   "Default"
        FROM
        ALL_ARGUMENTS arg
        INNER JOIN ALL_OBJECTS func ON func.OBJECT_ID = arg.OBJECT_ID
        AND func.OBJECT_TYPE = 'PROCEDURE'
        WHERE arg.POSITION != 0 AND arg.OWNER NOT IN ('SYS','CTXSYS','SYSTEM','EXFSYS','ORDSYS','XDB','ORDDATA','WMSYS','ORDPLUGINS','MDSYS','OLAPSYS','WKSYS')
        UNION ALL
        SELECT
        '''' || ar.PACKAGE_NAME || '.' || ar.OBJECT_NAME || '_' || ar.ARGUMENT_NAME || ''''	"Id"
        ,	'''' || ar.PACKAGE_NAME || '.' || ar.OBJECT_NAME || ''''	"ParentId"
        ,	ar.ARGUMENT_NAME   "Name"
        ,	ar.POSITION	      "Ordinal"
        ,	LOWER(ar.DATA_TYPE)"TypeName"
        ,	ar.DATA_LENGTH     "MaxLength"
        ,	ar.DATA_PRECISION  "Precision"
        ,	ar.DATA_PRECISION	"DateTimePrecision"
        ,	ar.DATA_SCALE	    "Scale"
        ,	CAST(NULL as varchar2(1)) "CollationCatalog"
        ,	CAST(NULL as varchar2(1)) "CollationSchema"
        ,	CAST(NULL as varchar2(1)) "CollationName"
        ,	CAST(NULL as varchar2(1)) "CharacterSetCatalog"
        ,	CAST(NULL as varchar2(1)) "CharacterSetSchema"
        ,	CHARACTER_SET_NAME	"CharacterSetName"
        ,	0	                  "IsMultiSet"
        , (CASE WHEN INSTR(UPPER(IN_OUT), 'IN/OUT') != 0 THEN 'INOUT' ELSE IN_OUT END) "Mode"
        ,	DEFAULT_VALUE   "Default"
        FROM ALL_ARGUMENTS ar
        INNER JOIN ALL_PROCEDURES proc ON ar.OBJECT_NAME = proc.PROCEDURE_NAME
        WHERE PACKAGE_NAME IS NOT NULL AND
        ar.OBJECT_NAME NOT IN
        (SELECT OBJECT_NAME FROM ALL_ARGUMENTS WHERE POSITION = 0
        AND DATA_TYPE IS NOT NULL AND PACKAGE_NAME IS NOT NULL)
        AND (ar.OVERLOAD IS NULL OR ar.OVERLOAD = '1')
        AND ar.OWNER NOT IN ('SYS','CTXSYS','SYSTEM','EXFSYS','ORDSYS','XDB','ORDDATA','WMSYS','ORDPLUGINS','MDSYS','OLAPSYS','WKSYS')
        -->
      </DefiningQuery>
    </EntitySet>
    
    <EntitySet Name="SConstraints" EntityType="Self.Constraint">
      <DefiningQuery>
        <!-- MS OracleClient sample code
        SELECT
        '''' || con.TABLE_NAME || '_' || con.CONSTRAINT_NAME || '''' "Id"
        , '''' || con.TABLE_NAME || '''' "ParentId"
        , con.CONSTRAINT_NAME "Name"
        ,         CASE con.CONSTRAINT_TYPE
        WHEN 'P' THEN 'PRIMARY KEY'
        WHEN 'R' THEN 'FOREIGN KEY'
        WHEN 'U' THEN 'UNIQUE'
        WHEN 'C' THEN 'CHECK'
        END "ConstraintType"
        , CASE con.DEFERRABLE WHEN 'NOT DEFERRABLE' THEN 0 ELSE 1 END "IsDeferrable"
        , CASE con.DEFERRED WHEN 'IMMEDIATE' THEN 1 ELSE 0 END "IsInitiallyDeferred"
        FROM
        USER_CONSTRAINTS con
        INNER JOIN USER_OBJECTS tab ON con.TABLE_NAME = tab.OBJECT_NAME
        AND tab.OBJECT_TYPE='TABLE' and con.TABLE_NAME  IS NOT NULL
        -->
        SELECT
        '''' || con.TABLE_NAME || '_' || con.CONSTRAINT_NAME || '''' "Id"
        , '''' || con.TABLE_NAME || '''' "ParentId"
        , con.CONSTRAINT_NAME "Name"
        ,         CASE con.CONSTRAINT_TYPE
        WHEN 'P' THEN 'PRIMARY KEY'
        WHEN 'R' THEN 'FOREIGN KEY'
        WHEN 'U' THEN 'UNIQUE'
        WHEN 'C' THEN 'CHECK'
        END "ConstraintType"
        , CASE con.DEFERRABLE WHEN 'NOT DEFERRABLE' THEN 0 ELSE 1 END "IsDeferrable"
        , CASE con.DEFERRED WHEN 'IMMEDIATE' THEN 1 ELSE 0 END "IsInitiallyDeferred"
        FROM ALL_CONSTRAINTS con, ALL_OBJECTS tab
        WHERE con.TABLE_NAME = tab.OBJECT_NAME
        AND tab.OBJECT_TYPE='TABLE' and con.TABLE_NAME IS NOT NULL
        AND tab.OWNER IN (SYS_CONTEXT('USERENV', 'CURRENT_USER'))
      </DefiningQuery>
    </EntitySet>

    <EntitySet Name="SCheckConstraints" EntityType="Self.CheckConstraint">
      <DefiningQuery>
        <!-- MS OracleClient sample code
        SELECT
        '''' || cc.TABLE_NAME || '_' || cc.CONSTRAINT_NAME || '''' "Id"
        , cc.SEARCH_CONDITION   "Expression"
        FROM
        USER_CONSTRAINTS cc
        WHERE cc.CONSTRAINT_TYPE = 'C'
        -->
        SELECT
        '''' || cc.TABLE_NAME || '_' || cc.CONSTRAINT_NAME || '''' "Id"
        , cc.SEARCH_CONDITION   "Expression"
        FROM ALL_CONSTRAINTS cc
        WHERE cc.CONSTRAINT_TYPE = 'C'
        AND cc.OWNER IN (SYS_CONTEXT('USERENV', 'CURRENT_USER'))
      </DefiningQuery>
    </EntitySet>

    <EntitySet Name="SConstraintColumns" EntityType="Self.ConstraintColumn">
      <DefiningQuery>
        <!-- MS OracleClient sample code
        SELECT
            '''' || col.TABLE_NAME || '_' || col.CONSTRAINT_NAME || ''''  "ConstraintId"
          , '''' || col.TABLE_NAME || '_' || col.COLUMN_NAME || ''''      "ColumnId"
        FROM USER_CONS_COLUMNS col
        -->
        SELECT
        '''' || col.TABLE_NAME || '_' || col.CONSTRAINT_NAME || ''''  "ConstraintId"
        , '''' || col.TABLE_NAME || '_' || col.COLUMN_NAME || ''''    "ColumnId"
        FROM ALL_CONS_COLUMNS col
        WHERE col.OWNER IN (SYS_CONTEXT('USERENV', 'CURRENT_USER'))
      </DefiningQuery>
    </EntitySet>

    <EntitySet Name="SForeignKeyConstraints" EntityType="Self.ForeignKeyConstraint">
      <DefiningQuery>
        <!-- MS OracleClient sample code
        SELECT
            '''' || TABLE_NAME || '_' || CONSTRAINT_NAME || ''''  "Id"
          , 'NO ACTION' "UpdateRule"
          , DELETE_RULE "DeleteRule"
        FROM
        USER_CONSTRAINTS
        WHERE
          CONSTRAINT_TYPE = 'R'
        -->
        SELECT
        '''' || TABLE_NAME || '_' || CONSTRAINT_NAME || ''''  "Id"
        , 'NO ACTION' "UpdateRule"
        , DELETE_RULE "DeleteRule"
        FROM ALL_CONSTRAINTS
        WHERE CONSTRAINT_TYPE = 'R'
        AND OWNER IN (SYS_CONTEXT('USERENV', 'CURRENT_USER'))
      </DefiningQuery>
    </EntitySet>
  
    <EntitySet Name="SForeignKeys" EntityType="Self.ForeignKey">
      <DefiningQuery>
        <!-- MS OracleClient sample code
        SELECT
        '''' || FK.TABLE_NAME || '_' || FK.CONSTRAINT_NAME || '_' || CAST(FK.POSITION as varchar2(30)) || '''' "Id"
        , '''' || PK.TABLE_NAME || '_' || PK.COLUMN_NAME || ''''      "ToColumnId"
        , '''' || FK.TABLE_NAME || '_' || FK.COLUMN_NAME || ''''      "FromColumnId"
        , '''' || FK.TABLE_NAME || '_' || FK.CONSTRAINT_NAME || ''''  "ConstraintId"
        , FK.POSITION "Ordinal"
        FROM USER_CONS_COLUMNS FK
        INNER JOIN USER_CONSTRAINTS FC ON FK.CONSTRAINT_NAME = FC.CONSTRAINT_NAME
        AND FC.CONSTRAINT_TYPE = 'R'
        INNER JOIN USER_CONS_COLUMNS PK ON PK.CONSTRAINT_NAME = FC.R_CONSTRAINT_NAME
        -->
        SELECT
        '''' || FK.TABLE_NAME || '_' || FK.CONSTRAINT_NAME || '_' || CAST(FK.POSITION as varchar2(30)) || '''' "Id"
        , '''' || PK.TABLE_NAME || '_' || PK.COLUMN_NAME || ''''      "ToColumnId"
        , '''' || FK.TABLE_NAME || '_' || FK.COLUMN_NAME || ''''      "FromColumnId"
        , '''' || FK.TABLE_NAME || '_' || FK.CONSTRAINT_NAME || ''''  "ConstraintId"
        , FK.POSITION "Ordinal"
        FROM ALL_CONS_COLUMNS FK, ALL_CONSTRAINTS FC, ALL_CONS_COLUMNS PK
        WHERE FK.CONSTRAINT_NAME = FC.CONSTRAINT_NAME AND FC.CONSTRAINT_TYPE = 'R'
        AND PK.CONSTRAINT_NAME = FC.R_CONSTRAINT_NAME AND
        PK.POSITION = FK.POSITION AND
        PK.OWNER IN (SYS_CONTEXT('USERENV', 'CURRENT_USER'))
      </DefiningQuery>
    </EntitySet>

    <EntitySet Name="SViewConstraints" EntityType="Self.ViewConstraint">
      <DefiningQuery>
        SELECT
        CAST(NULL as varchar2(1))     "Id"
        , CAST(NULL as varchar2(256)) "ParentId"
        , CAST(NULL as varchar2(256)) "Name"
        , CAST(NULL as varchar2(256)) "ConstraintType"
        , 0 "IsDeferrable"
        , 0 "IsInitiallyDeferred"
        , CAST(NULL as varchar2(11))  "Expression"
        , 'NO ACTION'  "UpdateRule"
        , 'NO ACTION'  "DeleteRule"
        FROM DUAL
        WHERE 1=2
