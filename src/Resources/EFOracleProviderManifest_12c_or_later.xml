    <!-- shsu: added float -->
    <!-- shsu: support ANSI type
         FLOAT = FLOAT (126) = 126 x 0.30103 = approx 37.9 digits of precision.
         Put maximum precision 126 here will allow EDM wizard to display columns of
         float or float(126) where 126 is the maximum. Scale is between -84 and 127.
    -->
    <Type Name="float" PrimitiveTypeKind="Decimal" >
      <FacetDescriptions>
        <Precision Minimum="0" Maximum="126" Constant="false" DefaultValue="0" />
        <Scale Minimum="0" Maximum="38" Constant="false" DefaultValue="0" />
